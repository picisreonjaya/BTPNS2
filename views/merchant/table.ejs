<!-- DataTales Example -->
<div class="card shadow mb-4">
  <div class="card-header py-3">
    <!-- Button trigger modal -->
  </div>
  <div class="card-body">
    <div class="table-responsive">
      <table
        class="table table-bordered"
        id="dataTable"
        width="100%"
        cellspacing="0"
      >
        <thead>
          <tr>
            <th>ID</th>
            <th>Nama</th>
            <th>Saldo</th>
          </tr>
        </thead>
        <tbody>
          <% for(let i = 0; i < MerchantList.length; i++){ %>
          <tr>
            <td><%= MerchantList[i].id %></td>
            <td><%= MerchantList[i].name_ %></td>
            <td><%= MerchantList[i].Saldo %></td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </div>
  </div>
</div>